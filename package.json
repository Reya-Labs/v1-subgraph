{
  "name": "v1-subgraph",
  "version": "1.0.0",
  "description": "A TypeScript graph interface for Voltz smart contract",
  "main": "dist/index.js",
  "types": "dist/types",
  "license": "MIT",
  "files": [
    "src/**/*",
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "prepare": "husky install && chmod ug+x .husky/*",
    "create": "graph create nicholaspiano/voltz-local-development --node https://api.thegraph.com/deploy/",
    "create-local": "graph create nicholaspiano/voltz-local-development --node http://127.0.0.1:8020",
    "remove-local": "graph remove nicholaspiano/voltz-local-development --node http://127.0.0.1:8020",
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy nicholaspiano/voltz-local-development --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "deploy-local": "graph deploy nicholaspiano/voltz-local-development --ipfs http://localhost:5001 --node http://127.0.0.1:8020",
    "prettier": "prettier 'src/**/*.ts'",
    "prettier:fix": "prettier --write 'src/**/*.ts'",
    "eslint": "eslint 'src/**/*.ts'",
    "eslint:fix": "eslint --fix 'src/**/*.ts'",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "release": "semantic-release"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@graphprotocol/graph-cli": "^0.26.0",
    "@graphprotocol/graph-ts": "^0.24.1",
    "@semantic-release/git": "^10.0.0",
    "@semantic-release/npm": "^8.0.2",
    "@types/faker": "^5.5.8",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.174",
    "@types/luxon": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "4.31.2",
    "@typescript-eslint/parser": "4.31.2",
    "commitizen": "^4.2.4",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "^7.26.0",
    "husky": "^7.0.2",
    "jest": "^27.2.2",
    "lint-staged": "11.1.2",
    "prettier": "2.4.1",
    "semantic-release": "^18.0.0",
    "semantic-release-cli": "^5.4.4",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  },
  "dependencies": {},
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix"
    ]
  },
  "publishConfig": {
    "access": "restricted"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "dist/**/*.{js,css}",
            "docs",
            "package.json"
          ],
          "message": "chore: Release ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
